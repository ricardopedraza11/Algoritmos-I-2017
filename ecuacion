#include "stdafx.h"
#include <iostream>
#include"conio.h"
using namspace std;
void main()
{
	float a,b,c,x1,x2,i,j,h,k;
	cout<<"para la ecuacion:ax^2+bx+c=0"<<endl;
	cout<<"ingrese el valor de a:";
	cin>>a;
	cout<<"ingrese el  valor de b:";
	cin>>c;
	h=((b*b)-(4*a*c));
	i=sqrt(h)
		j=i-b;
	x1=j/(2*a);
	k=-i-b;
	x2=k/(2*a);
	if (h<0)
		cout<<"la ecuacion no tiene solucion"
	else
	{
		cout<<" los resultados son:"<<endl;
		cout<<"x1="<<x1<<endl;
		cout<<"x2="<<x2<<endl;
	}
	getch();
}
